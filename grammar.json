{
  "start": "Program",

  "nonterminals": [
    "Program",
    "LambdaTerm"
  ],

  "terminals": [
    "lam",
    "{",
    "}",
    "Variable"
  ],

  "terminal2regex": {
    "Variable": "([a-z]|[A-z]|_)([a-z]|[A-z]|[0-9]|_)*"
  },

  "productions": {
    "Program": [ "LambdaTerm" ],
    "LambdaTerm": [ "lam <Variable> { <LambdaTerm> }" ]
  }
}